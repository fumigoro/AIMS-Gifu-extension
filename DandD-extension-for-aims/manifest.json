{
    "name": "Drag and Drop Extension for AIMS",
    "version": "0.0.4",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["https://aims2.gifu-u.ac.jp/courses/*"],
        "js": ["contents.js"],
        "css": ["style.css"]
      }
    ],
    "description" : "AIMS-Gifuの課題提出ページにドラッグアンドドロップでファイルをアップロードするフィールドを追加する拡張",
    "icons" : {
      "16" : "icons/icon16.png",
      "48" : "icons/icon48.png",
      "128" : "icons/icon128.png"
    },
    "permissions": ["https://aims2.gifu-u.ac.jp/courses/*"]

  }