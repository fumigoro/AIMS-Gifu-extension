{
    "name": "AIMS-Gifu Extension",
    "version": "2.0.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["https://aims2.gifu-u.ac.jp/courses/*"],
        "js": ["contents.js"],
        "css": ["style.css"]
      }
    ],
    "description" : "AIMS-Gifuを便利にする拡張機能",
